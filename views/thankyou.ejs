<div class="thank-you">
  <h2 id="message"></h2>
</div>

<script>
  const msg = "Thank you for showing interest. Our team will contact you soon...";
  let i = 0;
  function typeWriter() {
    if (i < msg.length) {
      document.getElementById("message").innerHTML += msg.charAt(i);
      i++;
      setTimeout(typeWriter, 50);
    } else {
      setTimeout(() => {
        window.location.href = "/classes";
      }, 3000);
    }
  }
  typeWriter();
</script>
